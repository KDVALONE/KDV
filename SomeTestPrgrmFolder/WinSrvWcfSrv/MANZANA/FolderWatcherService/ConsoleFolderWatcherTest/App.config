<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
  <section name="log4net"
           type="log4net.Config.Log4NetConfigurationSectionHandler,log4net, Version=1.2.15.0, Culture=neutral, PublicKeyToken=669e0ddf0bb1aa2a" />
  </configSections>   
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <appSettings>
    <add key="" value=""/>
    <add key="InputFolder" value="F:\\FolderWatcherServiceTest\Input"/>
    <add key="GarbageFolder" value="F:\\FolderWatcherServiceTest\Garbage"/>
    <add key="CompleteFolder" value="F:\\FolderWatcherServiceTest\Complete"/>
    <add key="SavedCheqesFolder" value="F:\\FolderWatcherServiceTest\SavedCheqes"/>
    <add key="LogFolder" value="F:\\FolderWatcherServiceTest\Logs"/>
    <add key="ChequesPackCount"     value="4"/>
  </appSettings>

  <!--CONSOLE FOLDER WATCHER TEST--> 
<log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <!--<param name="File" value="${LOCALAPPDATA}\FolderWatcherServiceTest\LogsTest\Example.log"/>-->
      <param name="File" value="%property{LogPath}\FolderWatcherService.log"/>
      <param name="AppendToFile" value="true"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="5MB"/>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <layout type="log4net.Layout.PatternLayout">
        <!--<param name="ConversionPattern" value="%d  %-5p %m%n"/>-->
        <conversionPattern value="%date %level [%thread] %logger - %message%newline"/>
      </layout>
      
      <!--<filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR"/>
        <levelMax value="FATAL"/>
      </filter>-->
    </appender>
  
    <!--<appender name="ErrorFile" type="log4net.Appender.RollingFileAppender">
      <file value="${LOCALAPPDATA}\FolderWatcherServiceTest\LogsTest\ExampleError.log"/>
      <appendToFile value="true"/>
      <maximumFileSize value="5MB"/>
      <maxSizeRollBackups value="10"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%d %level thread:%thread %logger - %message%newline"/>
      </layout>
      
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR"/>
        <levelMax value="FATAL"/>
      </filter>
    </appender>-->
  
    <logger name="MyLoggerTest">
      <appender-ref ref="LogFileAppender"/>
      <!--<appender-ref ref="ErrorFile"/>-->
    </logger>

</log4net>


  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IChequeServiceContract" />
      </basicHttpBinding>
      <netTcpBinding>
        <binding name="NetTcpBinding_IChequeServiceContract">
          <security>
            <transport sslProtocols="None" />
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:9001/WcfServiceHostService"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IChequeServiceContract"
        contract="WcfEchoServiceReferenceTest.IChequeServiceContract"
        name="BasicHttpBinding_IChequeServiceContract" />
      <endpoint address="net.tcp://localhost:9002/WcfServiceHostService"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IChequeServiceContract"
        contract="WcfEchoServiceReferenceTest.IChequeServiceContract"
        name="NetTcpBinding_IChequeServiceContract">
        <identity>
          <servicePrincipalName value="host/Dmitriy" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>