<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" 
             type="log4net.Config.Log4NetConfigurationSectionHandler,log4net, Version=1.2.15.0, Culture=neutral, PublicKeyToken=669e0ddf0bb1aa2a" />
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <appSettings>
    <add key="" value=""/>
    <add key="InputFolder" value="F:\\Temp"/>
    <add key="GarbageFolder" value="F:\\Garbage"/>
    <add key="CompleteFolder" value="F:\\Complete"/>
  </appSettings>
  
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString"
            value="%envFolderPath{CommonApplicationData}\\FolderWatcherSerivece\Log\Executing.log" />
      
      <param name="AppendToFile" value="true"/>
      <maxSizeRollBackups value="10"/>
      <maximumFileSize value="5MB"/>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level [%thread] %logger - %message%newline"/>
      </layout>
    </appender>

    <logger name="MyLogger">
      <appender-ref ref="LogFileAppender"/>
    </logger>
  </log4net>
  
  
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IChequeServiceContract" />
            </basicHttpBinding>
            <!--<netTcpBinding>
                <binding name="NetTcpBinding_IChequeServiceContract">
                    <security>
                        <transport sslProtocols="None" />
                    </security>
                </binding>
            </netTcpBinding>-->
        </bindings>
        <client>
            <endpoint address="http://localhost:9001/WcfServiceHostService"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IChequeServiceContract"
                contract="WcfEchoServiceReference.IChequeServiceContract"
                name="BasicHttpBinding_IChequeServiceContract" />
            <!--<endpoint address="net.tcp://localhost:9002/WcfServiceHostService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IChequeServiceContract"
                contract="WcfEchoServiceReference.IChequeServiceContract"
                name="NetTcpBinding_IChequeServiceContract">
                <identity>
                    <servicePrincipalName value="host/Dmitriy" />
                </identity>
            </endpoint>-->
        </client>
    </system.serviceModel>
  
</configuration>