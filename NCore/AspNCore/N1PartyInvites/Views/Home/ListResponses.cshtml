@model IEnumerable<N1PartyInvites.Models.GuestResponse>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-vidth" />
    <title>Response</title> 
</head>
<body>
    <h2> Here is the list of people attendibg the party</h2>
    <table>
        <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
        </thead>
        <tbody>
            @foreach (N1PartyInvites.Models.GuestResponse r in Model)
            { 
            <tr>
                <td>@r.Name</td>
                <td>@r.Email</td>
                <td>@r.Phone</td>
            </tr>
            }
        </tbody>
        </table>
</body>
</html>
